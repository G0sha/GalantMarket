    <div class="navbar-header">
      <a class="navbar-brand navbar-custom-brand" href="#">GALANT</a>
    </div>
    <div class="collapse navbar-collapse navbar-right">
      <div class="navbar-right navbar-brand navbar-custom-brand">8 (987) 654-32-10</div>
    </div>
  </div>
</div>
<div class="navbar navbar-inverse navbar-inverse-custom container">
  <ul class="nav navbar-nav">
    <li><%= link_to "Главная",  products_path, title: 'Главная' %></li>
    <li><%= link_to "Каталог",  products_path, title: 'Каталог' %></li>
    <li><%= link_to "Услуги",   "#" , title: 'Услуги' %></li>
    <li><%= link_to "Контакты", "#" , title: 'Контакты' %></li>
